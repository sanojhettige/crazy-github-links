{
  "storage": {
    "gitlinkstable": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "reposFunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "gitlinkstable",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    }
  },
  "api": {
    "reposApi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "reposFunction",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}